    <package name="default" namespace="/" extends="struts-default">
        <default-action-ref name="index" />
<action name="test_*_*" class="com.test.action.{1}" method="{2}">		      
            <result name="success">
                /{1}.jsp
            </result>
            <result name="stream" type="stream">
                
            </result>
            <result name="text">
                
            </result>
         <result name="loadtest">
            
            </result>
        </action>
  
    </package>


    <!-- Add packages here -->

</struts>
